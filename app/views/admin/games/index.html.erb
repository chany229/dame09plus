<h1>Listing admin_games</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Js file</th>
    <th>Width</th>
    <th>Height</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @games.each do |game| %>
  <tr>
    <td><%= game.title %></td>
    <td><%= game.js_file %></td>
    <td><%= game.width %></td>
    <td><%= game.height %></td>
    <td><%= link_to 'Show', [:admin, game] %></td>
    <td><%= link_to 'Edit', edit_admin_game_path(game) %></td>
    <td><%= link_to 'Destroy', [:admin, game], method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Game', new_admin_game_path %>
